{
  "name": "JeMarche",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "emulator": "emulator -list-avds | head -n1 | xargs emulator -avd",
    "eslint": "eslint . --ext js,ts,tsx --max-warnings 0",
    "eslint:write": "yarn eslint --fix",
    "jest": "jest",
    "open:android": "open -a \"/Applications/Android Studio.app\" android",
    "open:ios": "open \"ios/JeMarche.xcworkspace\"",
    "preflight": "yarn tsc && yarn eslint",
    "run:android": "react-native run-android --variant=devDebug --appId fr.en_marche.jecoute.dev",
    "run:ios": "react-native run-ios --simulator \"iPhone Xs\"",
    "simulator": "open -a Simulator",
    "start": "react-native start",
    "tsc": "tsc --build",
    "tsc:watch": "yarn tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn preflight"
    }
  },
